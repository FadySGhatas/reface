{
    "manifest_version": 2,
 
    "name": "Reface",
    "description": "How facebook should have been like.",
    "version": "1.0",
    "content_scripts": [{
        "matches": ["http://facebook.com/*", "https://facebook.com/*","http://*.facebook.com/*", "https://*.facebook.com/*"],
        "css": ["customStyle.css"]
        }],
    "permissions": ["http://facebook.com/*", "https://facebook.com/*","http://*.facebook.com/*", "https://*.facebook.com/*"]
}